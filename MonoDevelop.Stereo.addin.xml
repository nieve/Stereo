<Addin namespace   = "MonoDevelop"
       id          = "Stereo.CodeNavigation"
       name        = "Date Inserter"
       author      = "Nieve Goor"
       copyright   = "MIT/X11"
       url         = "http://www.monodevelop.com"
       description = "Provides a command to navigate to the base class."
       category    = "Source Editor Extensions"
       version     = "0.01">

    <Runtime>
		<Import assembly = "MonoDevelop.Refactoring.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Core" version="2.8.5"/>
		<Addin id="Ide" version="2.8.5"/>
		<Addin id="DesignerSupport" version="2.8.5"/>
		<Addin id="SourceEditor2" version="2.8.5"/>
	</Dependencies>

    <Extension path = "/MonoDevelop/Ide/Commands">
	
		<Category _name = "Code Navigation" id = "CodeNavigation">
		<Command id = "MonoDevelop.Refactoring.RefactoryCommands.GotoBase"
					defaultHandler = "MonoDevelop.Refactoring.GotoBaseHandler"
					_label = "Go to _base" 
					shortcut = "Alt|F12" />
<!--		<Command id = "MonoDevelop.Refactoring.RefactoryCommands.FindReferences"
					defaultHandler = "MonoDevelop.Refactoring.FindReferencesHandler"
					_label = "_Find references" 
					shortcut = "Shift|F12"
					macShortcut = "Shift|Meta|D"/>
		<Command id = "MonoDevelop.Refactoring.RefactoryCommands.FindDerivedClasses"
					defaultHandler = "MonoDevelop.Refactoring.FindDerivedClassesHandler"
					_label = "Find _derived classes" />-->
		</Category>
    </Extension>

<!--    <Extension path = "/MonoDevelop/Ide/MainMenu/Edit">
        <CommandItem id="MonoDevelop.Samples.DateInserter.DateInserterCommands.InsertDate" />
    </Extension>-->

</Addin>