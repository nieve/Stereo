<Addin namespace   = "MonoDevelop"
       id          = "Stereo.CodeNavigation"
       name        = "Date Inserter"
       author      = "Nieve Goor"
       copyright   = "MIT/X11"
       url         = "http://www.monodevelop.com"
       description = "Provides a command to navigate to the base class."
       category    = "Source Editor Extensions"
       version     = "0.01">

<!--    <Runtime>
		<Import assembly = "MonoDevelop.Refactoring.dll"/>
	</Runtime>-->

	<Dependencies>
		<Addin id="Core" version="2.8.5"/>
		<Addin id="Ide" version="2.8.5"/>
		<Addin id="DesignerSupport" version="2.8.5"/>
		<Addin id="SourceEditor2" version="2.8.5"/>
		<Addin id="Refactoring" version="2.8.5"/>
	</Dependencies>

    <Extension path = "/MonoDevelop/Ide/Commands">
	
		<Category _name = "Code Generation" id = "CodeGeneration">
		<Command id = "MonoDevelop.Refactoring.RefactoryCommands.MoveToAnotherFile"
					defaultHandler = "MonoDevelop.Stereo.Refactoring.MoveToAnotherFileHandler"
					_label = "Move to another _file" 
					shortcut = "Alt|Return" />
		</Category>
    </Extension>

    <Extension path = "/MonoDevelop/Ide/Commands/TextEditor">
		<Command id = "MonoDevelop.Stereo.TextEditor.Commands.DuplicateText"
					defaultHandler = "MonoDevelop.Stereo.TextEditor.DuplicateTextHandler"
					_label = "_Duplicate text" 
					shortcut = "Ctrl|Alt|D" />
    </Extension>
	
	<Extension path = "/MonoDevelop/Refactoring/Refactorings">
		<Class id="GenerateNewType" class = "MonoDevelop.Stereo.Refactoring.GenerateNewType.GenerateNewTypeRefactoring" />
		<Class id="RenameNamespace" class = "MonoDevelop.Stereo.Refactoring.Rename.RenameNamespaceRefactoring" />
	</Extension>
	
	<Extension path = "/MonoDevelop/Ide/Commands/Refactoring">
		<Command id = "MonoDevelop.Refactoring.RefactoryCommands.GenerateNewType"
				defaultHandler = "MonoDevelop.Stereo.Refactoring.GenerateClass.GenerateNewTypeHandler"
				_label = "Generate new type"/>
	</Extension>
	
<!--	<Extension path = "/MonoDevelop/AnalysisCore/FixHandlers">
		<Handler fixName="RenameNamespace" class = "MonoDevelop.Stereo.Refactoring.Rename.RenameNamespaceHandler" />
	</Extension>-->

</Addin>